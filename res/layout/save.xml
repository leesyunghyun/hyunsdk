<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#34ADD8E6"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="30"
        android:gravity="center"
        android:src="@drawable/save2" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="20"
        android:gravity="center"
        android:text="@string/exp2_10"
        android:textSize="15dp"
        android:textStyle="bold|italic"
        android:typeface="monospace" />

    <LinearLayout
        android:id="@+id/LinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="50"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="20"
            android:gravity="center" >

            <TextView
                android:id="@+id/save_tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="70dp"
                android:text="Storage1"
                android:layout_marginLeft="20dp"
                android:textColor="#993023ff"
                android:layout_weight="5"
                android:textStyle="italic"
                android:typeface="monospace" />

            <Button
                android:id="@+id/save_bt1"
                android:layout_width="43dp"
                android:layout_height="61dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/save_buttononoff"
                android:layout_weight="5"
                android:visibility="visible" />

            <Button
                android:id="@+id/sadelete_bt1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="5"
                android:background="@drawable/delete_buttononoff" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="70"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="T"
                    android:textSize="5dp"
                    android:visibility="invisible" />
            </LinearLayout>
        </LinearLayout>

